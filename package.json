{
  "name": "fallout-utility",
  "author": "FalloutStudios",
  "version": "2.7.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "default": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./Encryption": {
      "default": "./dist/scripts/classes/Encryption.js",
      "types": "./dist/scripts/classes/Encryption.d.ts"
    },
    "./Logger": {
      "default": "./dist/scripts/classes/Logger.js",
      "types": "./dist/scripts/classes/Logger.d.ts"
    },
    "./TypedEmitter": {
      "default": "./dist/scripts/classes/TypedEmitter.js",
      "types": "./dist/scripts/classes/TypedEmitter.d.ts"
    },
    "./cli": {
      "default": "./dist/scripts/cli.js",
      "types": "./dist/scripts/cli.d.ts"
    },
    "./commands": {
      "default": "./dist/scripts/commands.js",
      "types": "./dist/scripts/commands.d.ts"
    },
    "./numbers": {
      "default": "./dist/scripts/numbers.js",
      "types": "./dist/scripts/numbers.d.ts"
    },
    "./objects": {
      "default": "./dist/scripts/objects.js",
      "types": "./dist/scripts/objects.d.ts"
    },
    "./strings": {
      "default": "./dist/scripts/strings.js",
      "types": "./dist/scripts/strings.d.ts"
    },
    "./system": {
      "default": "./dist/scripts/system.js",
      "types": "./dist/scripts/system.d.ts"
    }
  },
  "keywords": [
    "fallout",
    "fallout-utility",
    "falloutstudios",
    "utility"
  ],
  "url": "http://github.com/FalloutStudios/Fallout-utility.git",
  "homepage": "https://github.com/FalloutStudios/Fallout-utility#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FalloutStudios/Fallout-utility.git"
  },
  "bugs": {
    "url": "https://github.com/FalloutStudios/Fallout-utility/issues"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npm run clean && npx tsc",
    "build:publish": "npm run build && npm publish"
  },
  "files": [
    "dist",
    "esm.mjs",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "kleur": "^4.1.5",
    "prompt-sync": "^4.2.0",
    "type-fest": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prompt-sync": "^4.2.0",
    "fallout-utility": "./",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
