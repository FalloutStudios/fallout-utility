{
  "name": "fallout-utility",
  "author": "Fallout Studios",
  "version": "1.5.3",
  "license": "MIT",
  "main": "./dist/cjs/index",
  "module": "./dist/mjs/index",
  "typings": "./dist/types/index",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "keywords": [
    "fallout",
    "fallout-utility",
    "falloutstudios"
  ],
  "url": "http://github.com/FalloutStudios/Fallout-utility.git",
  "homepage": "https://github.com/FalloutStudios/Fallout-utility#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FalloutStudios/Fallout-utility.git"
  },
  "bugs": {
    "url": "https://github.com/FalloutStudios/Fallout-utility/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run build:types && yarn run build:cjs && yarn run build:mjs",
    "build:types": "npx tsc -p ./tsconfig/tsconfig.types.json",
    "build:cjs": "npx tsc -p ./tsconfig/tsconfig.cjs.json",
    "build:mjs": "npx tsc -p ./tsconfig/tsconfig.mjs.json && echo '{ \"type\": \"module\" }' > ./dist/mjs/package.json",
    "build:publish": "yarn run build && yarn publish"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "prompt-sync": "^4.2.0",
    "strip-ansi": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/prompt-sync": "^4.1.1",
    "fallout-utility": "./",
    "typescript": "^4.7.4"
  }
}